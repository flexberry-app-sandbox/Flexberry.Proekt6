    }}
    {{flexberry-validationmessage error=(v-get validationModel "докУсловДог" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.докУсловДог.сумПоДогов
      class=(if (v-get validationModel.докУсловДог "сумПоДогов" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.докУсловДог.сумПоДогов-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-докУсловДог-сумПоДогов=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.докУсловДог "сумПоДогов" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.выполнение
      class=(if (v-get validationModel "выполнение" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.выполнение-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-выполнение=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "выполнение" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.сумКонечОстат
      class=(if (v-get validationModel "сумКонечОстат" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.сумКонечОстат-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-сумКонечОстат=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сумКонечОстат" "message")}}
  </div>
</form>

<h3 class="ui header">{{t "forms.i-i-s-proekt-от-по-вып-догов-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерДогово
      class=(if (v-get validationModel "номерДогово" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.номерДогово-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-номерДогово=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерДогово" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-от-по-вып-догов-e.объектСМР-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.объектСМР
      relationName="объектСМР"
      projection="ОбъектСМРL"
      title=(t "forms.i-i-s-proekt-от-по-вып-догов-e.объектСМР-caption")
      readonly=readonly
      componentName="объектСМРLookup"
      class=(if (v-get validationModel "объектСМР" "isInvalid") "error")
      data-test-i-i-s-proekt-от-по-вып-догов-e-объектСМР=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "объектСМР" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.объектСМР.объектСтроит
      class=(if (v-get validationModel.объектСМР "объектСтроит" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.объектСМР.объектСтроит-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-объектСМР-объектСтроит=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.объектСМР "объектСтроит" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-от-по-вып-догов-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      relationName="сотрудники"
      projection="ДолжностьL"
      title=(t "forms.i-i-s-proekt-от-по-вып-догов-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-proekt-от-по-вып-догов-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.фИО
      class=(if (v-get validationModel.сотрудники "фИО" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.сотрудники.фИО-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-сотрудники-фИО=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "фИО" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-от-по-вып-догов-e.спрКонтраг-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.спрКонтраг
      relationName="спрКонтраг"
      projection="СпрКонтрагL"
      title=(t "forms.i-i-s-proekt-от-по-вып-догов-e.спрКонтраг-caption")
      readonly=readonly
      componentName="спрКонтрагLookup"
      class=(if (v-get validationModel "спрКонтраг" "isInvalid") "error")
      data-test-i-i-s-proekt-от-по-вып-догов-e-спрКонтраг=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "спрКонтраг" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.спрКонтраг.наименование
      class=(if (v-get validationModel.спрКонтраг "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.спрКонтраг.наименование-caption")
      data-test-i-i-s-proekt-от-по-вып-догов-e-спрКонтраг-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.спрКонтраг "наименование" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-от-по-вып-догов-e.докСмета-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.докСмета
      relationName="докСмета"
      projection="ДокСметаL"
      title=(t "forms.i-i-s-proekt-от-по-вып-догов-e.докСмета-caption")
      readonly=readonly
      componentName="докСметаLookup"
      class=(if (v-get validationModel "докСмета" "isInvalid") "error")
      data-test-i-i-s-proekt-от-по-вып-догов-e-докСмета=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "докСмета" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.докСмета.обРабПоСмет
      class=(if (v-get validationModel.докСмета "обРабПоСмет" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-от-по-вып-догов-e.докСмета.обРабПоСмет-caption")

<h3 class="ui header">{{t "forms.i-i-s-proekt-расх-мат-в-стро-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.кодСДР
      class=(if (v-get validationModel "кодСДР" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.кодСДР-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-кодСДР=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "кодСДР" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.количество
      class=(if (v-get validationModel "количество" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.количество-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-количество=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "количество" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.ценаЗаЕд
      class=(if (v-get validationModel "ценаЗаЕд" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.ценаЗаЕд-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-ценаЗаЕд=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "ценаЗаЕд" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.суммаИтого
      class=(if (v-get validationModel "суммаИтого" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.суммаИтого-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-суммаИтого=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаИтого" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-расх-мат-в-стро-e.докСмета-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.докСмета
      relationName="докСмета"
      projection="ДокСметаL"
      title=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.докСмета-caption")
      readonly=readonly
      componentName="докСметаLookup"
      class=(if (v-get validationModel "докСмета" "isInvalid") "error")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-докСмета=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "докСмета" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.докСмета.обРабПоСмет
      class=(if (v-get validationModel.докСмета "обРабПоСмет" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.докСмета.обРабПоСмет-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-докСмета-обРабПоСмет=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.докСмета "обРабПоСмет" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-расх-мат-в-стро-e.номенклатура-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.номенклатура
      relationName="номенклатура"
      projection="НоменклатураL"
      title=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.номенклатура-caption")
      readonly=readonly
      componentName="номенклатураLookup"
      class=(if (v-get validationModel "номенклатура" "isInvalid") "error")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-номенклатура=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номенклатура" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.номенклатура.наименов
      class=(if (v-get validationModel.номенклатура "наименов" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.номенклатура.наименов-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-номенклатура-наименов=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.номенклатура "наименов" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-расх-мат-в-стро-e.едИзмерен-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.едИзмерен
      relationName="едИзмерен"
      projection="ЕдИзмеренL"
      title=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.едИзмерен-caption")
      readonly=readonly
      componentName="едИзмеренLookup"
      class=(if (v-get validationModel "едИзмерен" "isInvalid") "error")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-едИзмерен=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "едИзмерен" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.едИзмерен.наименование
      class=(if (v-get validationModel.едИзмерен "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-расх-мат-в-стро-e.едИзмерен.наименование-caption")
      data-test-i-i-s-proekt-расх-мат-в-стро-e-едИзмерен-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.едИзмерен "наименование" "message")}}
  </div>
</form>
